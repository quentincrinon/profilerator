{
  "manifest_version": 3,
  "name": "Profilerator",
  "version": "1.0.0",
  "description": "Generates a random profile with all the useful information (name, email, tax-id, phone number, etc.)",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon.png",
      "32": "favicon.png",
      "128": "favicon.png"
    }
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "128": "favicon.png"
  },
  "permissions": []
}
